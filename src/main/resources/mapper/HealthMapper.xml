<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.codeshu.mapper.HealthMapper">
    <select id="selectAll" resultType="com.codeshu.entity.Health">
        select * from health
    </select>
    <select id="selectById" parameterType="int" resultType="com.codeshu.entity.Health">
        select * from health where id = #{id}
    </select>
</mapper>
